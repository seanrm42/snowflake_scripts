use role accountadmin;
create or replace warehouse admin_wh;
use schema snowflake.account_usage;

select query_id
    ,user_name
    ,warehouse_name
    ,warehouse_size
    ,bytes_scanned
    ,bytes_spilled_to_remote_storage
    ,(bytes_spilled_to_remote_storage/bytes_scanned) spilling_read_ratio
from query_history
where bytes_spilled_to_remote_storage > 0
order by spilling_read_ratio;
